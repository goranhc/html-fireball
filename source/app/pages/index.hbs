                    </figure>

                    {{/each}}
                </div>

            </div>

            <div class="col-lg-5 ">
                <img src="{{data.img}}demo/img-5.png" alt="" class="img-fluid position-relative z-5">
            </div>


        </div>
    </div>
</section>
